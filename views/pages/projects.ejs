<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/_head') %>
  </head>
  <body>
    <!-- Navigation -->
    <%- include('../partials/_nav') %> 
    <br>

    <div class="container">
    <!-- Project 1 -->
      <div class="slideshow-1 card p-5 bg-dark">
        <div class="slideshow-1-main">
          <div class="slides">
            <div class="numbertext">1/6</div>
            <img src="img/photos/cabana_04.jpg" style="width:100%">
          </div>
          <div class="slides">
            <div class="numbertext">2/6</div>
            <img src="img/photos/cabana_05.jpg" style="width:100%">
          </div>
          <div class="slides">
            <div class="numbertext">3/6</div>
            <img src="img/photos/cabana_06.jpg" style="width:100%">
          </div>
          <div class="slides">
            <div class="numbertext">4/6</div>
            <img src="img/photos/cabana_07.jpg" style="width:100%">
          </div>
          <div class="slides">
            <div class="numbertext">5/6</div>
            <img src="img/photos/cabana_08.jpg" style="width:100%">
          </div>
          <div class="slides">
            <div class="numbertext">6/6</div>
            <img src="img/photos/cabana_09.jpg" style="width:100%">
          </div>
        </div>


        <a class="prev" onclick="plusSlides(-1)">&#10094</a>
        <a class="next" onclick="plusSlides(1)">&#10095</a>

        <br>
        <div class="row">
          <div class="column">
            <img src="img/photos/cabana_04.jpg" class="demo cursor" onclick="currentSlide(1)">
          </div>
          <div class="column">
            <img src="img/photos/cabana_05.jpg"  class="demo cursor" onclick="currentSlide(2)">
          </div>
          <div class="column">
            <img src="img/photos/cabana_06.jpg"  class="demo cursor" onclick="currentSlide(3)">
          </div>
          <div class="column">
            <img src="img/photos/cabana_07.jpg"  class="demo cursor" onclick="currentSlide(4)">
          </div>
          <div class="column">
            <img src="img/photos/cabana_08.jpg" class="demo cursor" onclick="currentSlide(5)">
          </div>
          <div class="column">
            <img src="img/photos/cabana_09.jpg" class="demo cursor" onclick="currentSlide(6)">
          </div>
        </div>
      </div>
      <br>
      <!-- Project 2 -->
      <div id="slideshow-2" class="carousel slide" data-interval="false">
        <ol class="carousel-indicators">
          <li data-target="#slideshow-2" data-slide-to="0" class="active"></li>
          <li data-target="#slideshow-2" data-slide-to="1"></li>
          <li data-target="#slideshow-2" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-100" src="img/photos/patio_01.jpg">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="img/photos/patio_02.jpg">
          </div>
          <div class="carousel-item">
            <img class="d-block w-100" src="img/photos/patio_05.jpg">
          </div>
        </div>
        <a class="carousel-control-prev" href="#slideshow-2" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#slideshow-2" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>

    <!-- Footer -->
    <%- include('../partials/_footer') %>
    

  </body>
  <script src="js/projects.js"></script>
  <%- include('../partials/_scripts') %>
</html>
